<!DOCTYPE html>
<html>
  <head>
    <title>Butterflyr</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <script type="text/javascript">
  window.butterFree = "<%= image_url('butterfree.gif') %>";
  window.butterFlyLogo = "<%= image_url('butterfly_logo.png') %>";
  window.globeicon =  "<%= image_url("globeicon.png") %>";
  window.clock = "<%= image_url("clock.svg") %>";  
  window.geo = "<%= image_url("geo.svg") %>";
  window.house = "<%= image_url("house.svg") %>";
  window.abouticon = "<%= image_url("about-icon.jpg") %>";
  window.camera = "<%= image_url("camera.png") %>";
  window.pickle = "<%= image_url("pickle.png") %>"
  window.appacademy = "<%= image_url("appacademy") %>"
    </script>
  </head>
  <body>
    <%= yield %>
  </body>
</html>
